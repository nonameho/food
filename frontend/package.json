{
  "name": "food-ordering-frontend",
  "version": "1.0.0",
  "description": "Food ordering app web frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "build:strict": "tsc && vite build",
    "start": "node -e \"require('child_process').execSync('serve -s dist -p ' + (process.env.PORT || 8080) + ' -n', {stdio: 'inherit'})\"",
    "preview": "serve -s dist",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "axios": "^1.6.5",
    "socket.io-client": "^4.6.0",
    "zustand": "^4.4.7",
    "react-hook-form": "^7.49.3",
    "@hookform/resolvers": "^3.3.4",
    "zod": "^3.22.4",
    "react-toastify": "^9.1.3",
    "date-fns": "^3.0.6",
    "lucide-react": "^0.303.0",
    "uuid": "^10.0.0",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.33",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "prettier": "^3.1.1"
  }
}
